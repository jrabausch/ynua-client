{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./src",
    "./test"
  ]
}
